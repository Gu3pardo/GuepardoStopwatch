        </activity>
        <activity android:name="guepardoapps.guepardostopwatch.activities.ActivitySettings" >
        </activity>

        <service
            android:name="guepardoapps.guepardostopwatch.service.FloatingService"
            android:enabled="true" />
    </application>

</manifest>